{
    "include": [
        "inference/server"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/venv",
        "**/.venv"
    ],
    "typeCheckingMode": "basic",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.12",
    "extraPaths": [
        "inference/runner",
        "inference/runner/typings"
    ]
}