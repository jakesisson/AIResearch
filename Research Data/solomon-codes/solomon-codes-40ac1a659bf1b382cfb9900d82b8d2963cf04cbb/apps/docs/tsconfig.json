{
	"extends": "astro/tsconfigs/base",
	"include": [".astro/types.d.ts", "src/**/*"],
	"exclude": ["dist", "node_modules", "../../node_modules"],
	"compilerOptions": {
		"skipLibCheck": true,
		"noEmit": true,
		"strict": false,
		"moduleResolution": "bundler",
		"noImplicitAny": false,
		"allowJs": true,
		"isolatedModules": false
	}
}
