# Environment Variables for Cloudflare Deployment
# Copy this file to .env.local for local development

# Browserbase Configuration
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# GitHub OAuth Configuration
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# E2B Configuration (for code execution)
E2B_API_KEY=your_e2b_api_key_here

NEXT_PUBLIC_APP_URL=https://your-app-name.pages.dev
# Application Environment
NODE_ENV=production

# OpenTelemetry Configuration (Optional - Required in Production)
OTEL_EXPORTER_OTLP_ENDPOINT=https://your-otel-endpoint.com/v1/traces
OTEL_EXPORTER_OTLP_HEADERS={"authorization":"Bearer your_token"}
OTEL_SAMPLING_RATIO=0.1
OTEL_TIMEOUT=10000

# Application Configuration
APP_VERSION=1.0.0
SERVICE_NAME=solomon-codes-web
LOG_LEVEL=info
