        "directory": "packages/service-search"
    },
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/ChatLunaLab/chatluna/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "homepage": "https://github.com/ChatLunaLab/chatluna/tree/v1-dev/packages/service-search#readme",
    "scripts": {
        "build": "atsc -b"
    },
    "resolutions": {
        "@langchain/core": "0.3.62",
