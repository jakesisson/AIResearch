{
    "name": "koishi-plugin-chatluna-search-service",
    "description": "search support for chatluna",
    "version": "1.3.0-alpha.3",
    "main": "lib/index.cjs",
    "module": "lib/index.mjs",
    "typings": "lib/index.d.ts",
    "files": [
        "lib",
        "dist"
    ],
